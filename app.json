{
  "expo": {
    "name": "Shifu",
    "slug": "shifu",
    "version": "0.1.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "assetBundlePatterns": ["**/*"],
    "scheme": "shifu",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shifu.app",
      "buildNumber": "3",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Shifu needs your location to determine your local phase of the day (Morning, Afternoon, Evening) based on sun position."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#121212"
      },
      "package": "com.shifu.app",
      "versionCode": 3,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "WAKE_LOCK",
        "RECEIVE_BOOT_COMPLETED",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_DATA_SYNC"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "output": "single",
      "bundler": "metro"
    },
    "plugins": [
      "expo-sqlite",
      "onnxruntime-react-native",
      "expo-asset",
      "expo-background-task",
      "expo-task-manager"
    ],
    "transpilePackages": ["@huggingface/transformers", "@xenova/transformers"],
    "extra": {
      "eas": {
        "projectId": "65d41095-1adb-4070-b7ce-d9c2ea27b350"
      }
    }
  }
}
